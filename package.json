{
  "name": "sandbox",
  "version": "0.0.0",
  "description": "an api for computers",
  "scripts": {
    "typecheck": "turbo run typecheck",
    "format": "prettier --write .",
    "check": "sherif && biome check && turbo run typecheck",
    "fix": "biome check --fix && turbo run typecheck",
    "build": "turbo run build",
    "build:clean": "turbo run build --force",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "docker:rebuild": "npm run build:clean && npm run docker:local -w @cloudflare/sandbox"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "workspaces": [
    "packages/*",
    "tooling/*",
    "tests/integration",
    "examples/*"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.3.2",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@cloudflare/vite-plugin": "^1.13.17",
    "@cloudflare/vitest-pool-workers": "^0.10.2",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/bun": "^1.3.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^3.2.4",
    "fast-glob": "^3.3.3",
    "happy-dom": "^20.0.10",
    "pkg-pr-new": "^0.0.60",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sherif": "^1.7.0",
    "tsdown": "^0.15.11",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4",
    "wrangler": "^4.45.2",
    "ws": "^8.18.3"
  },
  "private": true,
  "packageManager": "npm@11.6.2"
}
